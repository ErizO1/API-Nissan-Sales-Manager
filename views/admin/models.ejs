<% include ../partials/header %>

<section class="main">
    <div class="heading container">
        <h1 class="heading__title">Vehicles</h1>
        <div class="heading__controls">
            <div class="table-filter__group">
                <div class="table-filter__item">
                    <p class="table-filter__title">Category</p>
                    <select class="modal-form__input modal-form__input--select" name="" id="filter-category">
                        <option value="All">All</option>    
                        <option value="Cars">Cars</option>
                        <option value="Pick-Up">Pick Up</option>
                        <option value="SUVS">SUVS</option>
                        <option value="Service">Service</option>
                    </select>
                </div>
            </div>
            <a class="table-filter__btn" href="#" data-toggle="modal" data-target=".add-model" id="add-model-btn">
                Add Vehicle
            </a>
        </div>
        <table class="table">
            <thead class="table__head">
                <tr class="table__row-th">
                    <th class="table__th table__th--is-active">MODEL <span class=""></span></th>
                    <th class="table__th">YEAR <span class=""></span></th>
                    <th class="table__th">VERSIONS <span class=""></span></th>
                    <th class="table__th">CATEGORY <span class=""></span></th>
                </tr>
            </thead>
            <tbody class="table__body" id="table-models">
            </tbody>
        </table>
    </div>

    <!--Add vehicle-->
