                                <input type="text" class="modal-form__input" name="name">
                            </div>
                            <div class="modal-form__group modal-form__group--minium">
                                <label for="" class="modal-form__label">Category</label>
                                <select class="modal-form__input" name="category" id="">
                                        <option value="Servicio">Serviciio</option>
                                        <option value="Auto">Auto</option>
                                        <option value="Pick Up">PickUp</option>
                                        <option value="SUV">SUV</option>
                                </select>								  
                            </div>
                            <div class="modal-form__group modal-form__group--minium">
                                <label for="" class="modal-form__label">Year</label>
                                <input type="text" class="modal-form__input" name="year">							  
                            </div>
                            <p class="modal-form__label modal-form__label--title">Dimensions</p>
                            <div class="modal-form__group modal-form__group--extra-small">
                                <label for="" class="modal-form__label">Height</label>
                                <input type="text" class="modal-form__input" name="dimensions[alto]">							  
                            </div>
